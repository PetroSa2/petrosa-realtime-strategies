{
  "dashboard": {
    "title": "Petrosa Realtime Strategies - Business Metrics",
    "description": "Custom business metrics for real-time strategy monitoring",
    "tags": ["petrosa", "realtime-strategies", "opentelemetry"],
    "timezone": "utc",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Message Processing Rate",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(realtime_messages_processed_total[1m])",
            "legendFormat": "{{symbol}} - {{message_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "msg/s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 100, "color": "yellow"},
                {"value": 1000, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Strategy Execution Latency (p95)",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(realtime_strategy_latency_bucket[1m]))",
            "legendFormat": "{{strategy}} - {{symbol}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 50, "color": "yellow"},
                {"value": 100, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "NATS Consumer Lag",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "realtime_consumer_lag",
            "legendFormat": "Consumer Lag",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 5, "color": "yellow"},
                {"value": 30, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Signals Generated (by Type)",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(realtime_signals_generated_total[1m])",
            "legendFormat": "{{strategy}} - {{signal_type}} ({{symbol}})",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "signals/s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "blue"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Strategy Success Rate",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(realtime_strategy_executions_total{result=\"success\"}[1m]) / rate(realtime_strategy_executions_total[1m])",
            "legendFormat": "{{strategy}} - {{symbol}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.7, "color": "yellow"},
                {"value": 0.9, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Error Rate",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(realtime_errors_total[1m])",
            "legendFormat": "{{error_type}} - {{strategy}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "errors/s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 0.1, "color": "yellow"},
                {"value": 1, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Message Types Distribution",
        "type": "piechart",
        "gridPos": {"x": 0, "y": 24, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "sum by(type) (rate(realtime_message_types_total[5m]))",
            "legendFormat": "{{type}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {"displayMode": "table", "placement": "right"},
          "pieType": "donut"
        }
      },
      {
        "id": 8,
        "title": "Signal Confidence Distribution",
        "type": "piechart",
        "gridPos": {"x": 6, "y": 24, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "sum by(confidence_bucket) (rate(realtime_signals_generated_total[5m]))",
            "legendFormat": "{{confidence_bucket}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {"displayMode": "table", "placement": "right"},
          "pieType": "donut"
        }
      },
      {
        "id": 9,
        "title": "Top Strategies by Signal Count",
        "type": "bargauge",
        "gridPos": {"x": 12, "y": 24, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by(strategy) (rate(realtime_signals_generated_total[5m]))",
            "legendFormat": "{{strategy}}",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "signals/s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "blue"},
                {"value": 0.5, "color": "green"},
                {"value": 1, "color": "yellow"}
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "Strategy Execution Count",
        "type": "stat",
        "gridPos": {"x": 0, "y": 32, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(rate(realtime_strategy_executions_total[1m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "exec/s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "blue"}
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "title": "Total Messages Processed",
        "type": "stat",
        "gridPos": {"x": 6, "y": 32, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(rate(realtime_messages_processed_total[1m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "msg/s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 12,
        "title": "Average Strategy Latency",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 32, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(realtime_strategy_latency_sum[1m]) / rate(realtime_strategy_latency_count[1m])",
            "legendFormat": "{{strategy}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 20, "color": "yellow"},
                {"value": 50, "color": "red"}
              ]
            }
          }
        }
      }
    ]
  }
}

